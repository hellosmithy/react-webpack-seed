  "license": "ISC",
  "version": "0.0.0",
  "description": "Flux+React Webpack Seed Project",
  "keywords": [
    "react",
    "flux",
    "webpack"
  ],
  "main": "app/app.js",
  "scripts": {
    "prebuild": "cleandir ./build",
    "build": "webpack --history-api-fallback",
    "postbuild": "cp ./app/htaccess ./build/.htaccess",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start",
    "dev": "NODE_ENV=development webpack-dev-server --devtool eval --progress --colors --hot --content-base build --history-api-fallback",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js"
  },
  "dependencies": {
    "alt": "^0.17.1",
    "classnames": "^2.1.3",
    "react": "^0.13.3",
    "react-router": "^1.0.0-beta3"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.0.10",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "chai-spies": "^0.7.0",
    "css-loader": "^0.16.0",
    "cssnext-loader": "^1.0.1",
    "eslint": "^1.2.0",
    "eslint-plugin-react": "^3.2.3",
    "html-webpack-plugin": "^1.6.1",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-osx-reporter": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "postcss-nested": "^0.3.2",
    "react-hot-loader": "^1.2.8",
    "sinon": "^1.15.4",
    "style-loader": "^0.12.3",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
